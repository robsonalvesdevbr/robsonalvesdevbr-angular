{
  "name": "robsonalvesdevbr-angular",
  "version": "0.0.0",
  "engines": {
    "node": ">=24.0"
  },
  "lint-staged": {
    "**/*": ["prettier --write", "biome check --write"]
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test-nowatch": "ng test --watch=false --no-progress --browsers=ChromeHeadless",
    "test-coverage": "ng test --no-watch --code-coverage",
    "prettier-write": "npx prettier ./src --write",
    "prettier-check": "npx prettier ./src --check",
    "biome": "npx @biomejs/biome check --write .",
    "prepare": "husky"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "19.2.10",
    "@angular/common": "19.2.10",
    "@angular/compiler": "19.2.10",
    "@angular/core": "19.2.10",
    "@angular/forms": "19.2.10",
    "@angular/platform-browser": "19.2.10",
    "@angular/platform-browser-dynamic": "19.2.10",
    "@angular/router": "19.2.10",
    "@hakimio/ngx-google-analytics": "15.0.0",
    "ngx-pagination": "6.0.3",
    "rxjs": "7.8.2",
    "tslib": "2.8.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.2.11",
    "@angular/cli": "19.2.11",
    "@angular/compiler-cli": "19.2.10",
    "@biomejs/biome": "1.9.4",
    "@types/jasmine": "5.1.8",
    "husky": "9.1.7",
    "jasmine-core": "5.7.1",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "lint-staged": "15.5.2",
    "prettier": "3.5.3",
    "typescript": "5.8.3"
  }
}
