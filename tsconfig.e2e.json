{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/e2e",
    "module": "commonjs",
    "target": "ES2021",
    "lib": ["ES2021"],
    "types": ["node", "@playwright/test"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true
  },
  "include": ["e2e/**/*.ts"]
}
